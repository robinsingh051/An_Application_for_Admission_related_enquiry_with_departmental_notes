<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginApp"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".sc"
            android:exported="false"
            android:label="Search" />
        <activity
            android:name=".chatbot"
            android:exported="true"
            android:label="Admission enquiry bot"/>
        <activity
            android:name=".subject"
            android:exported="false"
            android:label="Subject"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".pdfpage"
            android:exported="false"
            android:label="Notes" />
        <activity
            android:name=".module"
            android:exported="false"
            android:label="Module" />
        <activity
            android:name=".LoggedIn"
            android:exported="false"
            android:label="Dashboard" />
        <activity
            android:name=".sempage"
            android:exported="false"
            android:label="Semester" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.Noaction" />
        <activity
            android:name=".first"
            android:theme="@style/Theme.Noaction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Noaction"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>